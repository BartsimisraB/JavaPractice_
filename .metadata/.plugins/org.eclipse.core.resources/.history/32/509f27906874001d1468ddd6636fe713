package Day_10_Java;

public class Main5 {
	public static void main(String[] args) throws Exception {

		for (int i = 0; i < 4; i++) {
			
			for (int k = i; k < 4; k++) {
				System.out.print(" ");
			}

			for (int j = 0; j <= i*2; j++) {
				System.out.print(j);
			}
			System.out.println();
		}
	}
}