package Book;

public class Page_2_123_2_3 {
	public static void main(String[] args) {
		// 2.
		for (int i = 1; i <= 9; i++) {

			for (int j = 1; j <= 9; j++) {
				if (i % 2 == 0) {
					System.out.println(i + "X" + j + "=" + (i * j));
				} else {
					continue;
				}
			}
			System.out.println();
		}

		// 3.
		for (int i = 1; i <= 9; i++) {

			for (int j = 1; j <= i; j++) {
				System.out.println(i + "X" + j + "=" + (i * j));
			}
			System.out.println();
		}
	}
}
