package Day9_Java;

class Man {
	String name;
	int age;

	Man() {
		this("이름 없음", 1);
	}

	Man(String name, int age) {
		this.name = name;
		this.age = age;

	}
}

public class Excercise7 {
	public static void main(String[] args) {

		Man noName = new Man();
		System.out.println(noName.name);
		System.out.println(noName.age);
	}
}
