package Day_10_Java;

class Cal {
//	int a, b;

	static int add(int a, int b) {
		
		return a+b;
	}
	static int div(int a, int b) {
		
		return a/b;
	}

}

public class Main4 {
	public static void main(String[] args) throws Exception {

		int a = Cal.add(10, 5); // Че

		int b = Cal.div(10, 5); // Иђ

		System.out.println(a + " " + b);
	}
}