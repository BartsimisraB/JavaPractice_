package Day_10_Java;

import java.util.Scanner;

public class Main5 {
	private static int get(int[][] a) {
		int high = 0;
		int temp = 0;
		for(int i = 0;i<a.length; i++) {
			for(int j = 0; j<a.length-1;j++) {
				if(a[i][j]>a[i][j+1]) {
					temp = a[i][j];
					a[i][j] = a[i][j+1];
					a[i][j+1]=temp;
				}
			}
		}
		for (int i = 0; i < a.length; i++) {
			high = a[a.length - 1][a[i].length - 1];
		}
		return high;
	}

	public static void main(String[] args) throws Exception {

//		for (int i = 0; i < 4; i++) {
