//			for (int j = 0; j <= i*2; j++) {
//				System.out.print(j);
//			}
//			System.out.println();
//		}
		int[][] grade = { { 55, 60, 65 }, { 85, 90, 95 } };

		int high;

		high = get(grade);

		System.out.println("가장 높은 점수는 " + high + "점 입니다.");

Scanner sc = new Scanner(System.in);
String eng[]={"student","book","future","note"};

String kor[]={"학생","책","미래","노트"};

while(true) {
	System.out.println("영어 단어 입력 : ");
	String str_eng = sc.next();
	System.out.println("한글 단어 입력 : ");
	String str_kr =sc.next();
	if (str_eng.equalsIgnoreCase("student"))	System.out.println(kor[0]);
	else if(str_eng.equals("book"))	System.out.println(kor[1]);
	else if()
	if(str_eng.equals("stop"))	break;
	
	
