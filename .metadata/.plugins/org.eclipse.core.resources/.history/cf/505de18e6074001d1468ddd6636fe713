package Day_10_Java;
class Music {
	String title;
	String singer;
	
	Music(String a, String b){
		title = a;
		singer = b;
	}
	Music(String a){
		title = a;
	}
	
}


public class Main3 {
	public static void main(String[] args) throws Exception {

			
			Music m=new Music("On", "BTS");
			
			System.out.println(title+ " " +singer); // On , BTS 출력
			
			Music m1=new Music(“Butter”);
			
			System.out.println(title+" " +singer); // Butter, 방탄 출력Main.java
	}
}