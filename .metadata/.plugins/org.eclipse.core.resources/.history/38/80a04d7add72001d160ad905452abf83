//			}
//		}
		
		Scanner sc = new Scanner(System.in);
		int nums[] = new int[10];
		
		for (int num : nums) {
			if(num % 4 == 0) System.out.println(num);
		}
//	2. 정수 a=50, b=150으로 초기화해서 a가 b보다 크면 a와 b의 곱, 아니면 a를 b로 나눴을 때의
//	나머지 출력(삼항 조건 연산자 활용)

		int a = 50, b = 150;
		System.out.println(a > b ? a * b : a % b);
	}

}