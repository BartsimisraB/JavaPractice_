package Book;

public class Page_68 {
	public static void main(String[] args) {
		
		double dNum1 = 1.2;
		float fNum2 = 0.9F;
		
		int iNum3 = (int)dNum1 + (int)fNum2;	//두 실수가 각각 형변환 후 더함
		int iNum4 = (int)(dNum1 + fNum2);	//두 실수의 합을 형 변환
		System.out.println(iNum3);
		System.out.println(iNum4);
	}
}
