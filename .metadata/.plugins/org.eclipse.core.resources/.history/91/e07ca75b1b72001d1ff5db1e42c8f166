package Day9_Java;

import java.util.Scanner;

public class Excercise915 {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		
		int cnt=0,sum=0;
		
		while(true) {
			double a = sc.nextDouble();
			if(a == -1) {
				break;
			}
			cnt++;
			sum+=a;
			
		}
		System.out.println(sum/cnt);
	}
}
