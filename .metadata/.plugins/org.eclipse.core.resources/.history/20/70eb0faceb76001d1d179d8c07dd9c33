package A_stream;

import java.io.FileInputStream;

public class Page_4_525 {
	public static void main(String[] args) {
		FileInputStream fis = null;
		try {
			fis = new FileInputStream("input.txt");
			int i;
			while ((i = fis.read()) != -1) {
				System.out.println((char) i);
			}
			System.out.println("end");
			fis.close();
		} catch (Exception e) {
			e.printStackTrace();
		}
	}
}
