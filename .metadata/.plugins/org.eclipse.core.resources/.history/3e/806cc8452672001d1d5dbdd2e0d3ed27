package Day9_Java;

import java.io.FileWriter;
import java.util.Scanner;

public class A3 {

	public static void main(String[] args) throws Exception {

		Scanner sc = new Scanner(System.in);

		FileWriter fw = null;

		try {
			fw = new FileWriter("teste02.txt");
			while (true) {
				String str = sc.next();
				if (str.equals("그만")) {
					fw.write("그만\n");
					fw.close();
					sc.close();
					break;
				} else {
					String name = str;
					String id = sc.next();
					fw.write(name + " " + id + "\n");
				}
			}

		} catch (Exception e) {
			e.printStackTrace();
		}

	}
}
