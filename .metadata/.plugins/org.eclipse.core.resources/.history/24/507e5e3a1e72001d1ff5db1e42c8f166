package Day9_Java;

import java.io.BufferedReader;
import java.io.BufferedWriter;
import java.io.FileReader;

public class A4 {

	public static void main(String[] args) throws Exception {

		FileReader fr = null;
		try {
			fr = new FileReader("test02.txt");
			int r;
			while ((r = fr.read()) != -1) {
				System.out.print((char) r);
			}
		} catch (Exception e) {
			e.printStackTrace();
		}
		System.out.println();
	}
}
